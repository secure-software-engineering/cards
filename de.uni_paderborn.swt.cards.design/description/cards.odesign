                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:flowAssumption">
                        <subModelOperations xsi:type="tool_1:RemoveElement"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool_1:For" expression="aql:model.assumption.sanitizersAssumptions" iteratorName="sanitizersAssumption">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:sanitizersAssumption.source  = element or sanitizersAssumption.target  = element">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:sanitizersAssumption">
                        <subModelOperations xsi:type="tool_1:RemoveElement"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:Let" variableName="system" valueExpression="aql:element.ancestors() -> filter(tmdsl::System)">
                  <subModelOperations xsi:type="tool_1:For" expression="aql:system.eAllContents() -> filter(tmdsl::PortPart)" iteratorName="portPart">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:portPart.port = element">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:portPart">
                        <subModelOperations xsi:type="tool_1:For" expression="aql:system.eAllContents() -> filter(tmdsl::PortMapping)" iteratorName="portMapping">
                          <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:portMapping.target = portPart">
                            <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:portMapping">
                              <subModelOperations xsi:type="tool_1:RemoveElement"/>
                            </subModelOperations>
                          </subModelOperations>
                        </subModelOperations>
                        <subModelOperations xsi:type="tool_1:For" expression="aql:system.eAllContents() -> filter(tmdsl::PortConnector)" iteratorName="portConnector">
