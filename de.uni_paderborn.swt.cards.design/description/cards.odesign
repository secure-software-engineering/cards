            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:element.eContainer()">
                <subModelOperations xsi:type="tool_1:Let" variableName="model" valueExpression="aql:element.ancestors() -> filter(tmdsl::Model)">
                  <subModelOperations xsi:type="tool_1:For" expression="aql:model.assumption.portAssumptions" iteratorName="portAssumption">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="aql:portAssumption.port = element">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:portAssumption">
                        <subModelOperations xsi:type="tool_1:RemoveElement"/>
                      </subModelOperations>
                    </subModelOperations>
